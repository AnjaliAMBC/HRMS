<script>
    $('.emp-entertimesheet-rangedates span').on('click', function (event) {
        var clickedElement = $(event.target); // The element that was clicked
        console.log(clickedElement);
        console.log(clickedElement.attr('href')); // If the element has an href attribute
        var href = clickedElement.attr('href');

        $('.emp-entertimesheet-rangedates span').removeClass('active');
        clickedElement.addClass('active');

        $('.emp-entertimesheet-day-block').removeClass('active');
        $(href).addClass('active');
    });

    $('.emp-entertimesheet-footer .emp-entertimesheet-addtask button').on('click', function (event) {
        var className = $(this).attr('class');
        var lastChar = className.slice(-1); // Get the last character
        console.log(lastChar);

        // Check if row-6 is display:flex
        if ($('#emp-entertimesheet-day-block' + lastChar + ' .row-6').css('display') === 'flex') {
            // Change row-7 to display:flex
            $('#emp-entertimesheet-day-block' + lastChar + ' .row-7').css('display', 'flex');
        }

        $('#emp-entertimesheet-day-block' + lastChar + ' .row-6').css({
            'display': 'flex'
        });



    });

</script>